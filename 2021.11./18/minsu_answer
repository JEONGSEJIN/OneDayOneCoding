n = int(input())
print("n is :", n)
points = list(map(int, input().split()))
squeezed_points = []

for i in range(int(n)):
    temp = 0
    for j in range(n):
        if i == j:
            continue
        if points[i] == points[j]:
            continue
        if points[i] > points[j]:
            temp += 1
    squeezed_points.append(temp)

print(squeezed_points)
